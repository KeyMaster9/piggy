{
  "name": "piggy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=development webpack --watch --config webpack.config.js",
    "start:dev": "NODE_ENV=development nodemon -w views -w sass -w build build/app.js",
    "start:prod": "NODE_ENV=production node build/app.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0",
    "sequelize": "^5.10.1",
    "sqlite3": "^5.0.2",
    "twig": "~0.10.3",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1"
  },
  "nodemonConfig": {
    "watch": "views, routes",
    "ext": "js, mjs, json, twig"
  }
}
